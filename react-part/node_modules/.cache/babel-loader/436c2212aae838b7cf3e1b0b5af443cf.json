    this.setChartData(\"f\",timeline)\r\n        break;\r\n      case 'max':\r\n          console.log(\"----TIMELINE----\",timeline)\r\n        this.setChartData(\"f\",timeline)\r\n        break;\r\n      default:\r\n\r\n    }\r\n  }\r\n/*****************************************************************/\r\n\r\n  render() {\r\nconsole.log(\"RENDER period=>\", this.state.selection)\r\nconsole.log(\"RENDER pickSymbol=>\", this.props.pickedTicker)\r\nconsole.log(\"RENDER state series=>\", this.state.series)\r\nconsole.log(\"RENDER xaxis\",this.state.options.xaxis.min,this.state.options.xaxis.max, this.state.options )\r\n\r\n    return (\r\n      <div>\r\n        <div id=\"chart\">\r\n          <div className=\"toolbar\">\r\n            <button onClick={()=>this.updateData('1m')} id=\"1m\" className={ (this.state.selection==='1m' ? 'active' : '')}>1M</button>\r\n            <button onClick={()=>this.updateData('6m')} id=\"6m\" className={ (this.state.selection==='6m' ? 'active' : '')}>6M</button>\r\n            <button onClick={()=>this.updateData('1y')} id=\"1y\" className={ (this.state.selection==='1y' ? 'active' : '')}>1Y</button>\r\n            <button onClick={()=>this.updateData('ytd')} id=\"ytd\" className={ (this.state.selection==='ytd' ? 'active' : '')}>YTD</button>\r\n            <button onClick={()=>this.updateData('max')} id=\"max\" className={ (this.state.selection==='max' ? 'active' : '')}>ALL</button>\r\n          </div>\r\n          <ReactApexChart options={this.state.options} series={this.state.series} type=\"area\" height=\"350\" />\r\n        </div>\r\n        <div id=\"html-dist\"> \r\n\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default AreaChart;\r\n"]},"metadata":{},"sourceType":"module"}