n        }\r\n      }\r\n    })  \r\n  }\r\n\r\n  getChartData = (ticker, period) => {\r\n    const promise = fetch(`https://cloud.iexapis.com/stable/stock/${ticker}/chart/${period}?token=${process.env.REACT_APP_API_PUBLISH}`)\r\n    promise.then(blob=>blob.json()).then(json=>{\r\n      console.log(\"JSON\",json)\r\n      this.setChartData(json)\r\n    })\r\n  }\r\n\r\n\r\n\r\n  componentDidMount(){\r\n    console.log(\"==COMPONENT DID MOUNT===\")\r\n    this.getChartData(\"f\", \"1m\")\r\n  }\r\n  updateData (timeline) {\r\n    this.setState({\r\n      selection: timeline\r\n    })\r\n    \r\n    switch (timeline) {\r\n      \r\n      case '1m':\r\n          console.log(\"----TIMELINE----\",timeline)\r\n        this.setChartData(\"f\",timeline)\r\n        break;\r\n      case '6m':\r\n          console.log(\"----TIMELINE----\",timeline)\r\n        this.setChartData(\"f\",timeline)\r\n        break;\r\n      case '1y':\r\n          console.log(\"----TIMELINE----\",timeline)\r\n        this.setChartData(\"f\",timeline)\r\n        break;\r\n      case 'ytd':\r\n          console.log(\"----TIMELINE----\",timeline)\r\n        this.setChartData(\"f\",timeline)\r\n        break;\r\n      case 'max':\r\n          console.log(\"----TIMELINE----\",timeline)\r\n        this.setChartData(\"f\",timeline)\r\n        break;\r\n      default:\r\n\r\n    }\r\n  }\r\n/*****************************************************************/\r\n\r\n  render() {\r\nconsole.log(\"pickSymbol=>\", this.props.pickedTicker)\r\nconsole.log(\"state series=>\", this.state.series)\r\nconsole.log(\"xaxis\",this.state.options.xaxis.min,this.state.options.xaxis.max )\r\n\r\n    return (\r\n      <div>\r\n        <div id=\"chart\">\r\n          <div className=\"toolbar\">\r\n            <button onClick={()=>this.updateData('1m')} id=\"1m\" className={ (this.state.selection==='1m' ? 'active' : '')}>1M</button>\r\n            <button onClick={()=>this.updateData('6m')} id=\"6m\" className={ (this.state.selection==='6m' ? 'active' : '')}>6M</button>\r\n            <button onClick={()=>this.updateData('1y')} id=\"1y\" className={ (this.state.selection==='1y' ? 'active' : '')}>1Y</button>\r\n            <button onClick={()=>this.updateData('ytd')} id=\"ytd\" className={ (this.state.selection==='ytd' ? 'active' : '')}>YTD</button>\r\n            <button onClick={()=>this.updateData('max')} id=\"max\" className={ (this.state.selection==='max' ? 'active' : '')}>ALL</button>\r\n          </div>\r\n          <ReactApexChart options={this.state.options} series={this.state.series} type=\"area\" height=\"350\" />\r\n        </div>\r\n        <div id=\"html-dist\"> \r\n\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default AreaChart;\r\n"]},"metadata":{},"sourceType":"module"}